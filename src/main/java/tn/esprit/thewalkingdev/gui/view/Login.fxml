<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.thewalkingdev.gui.controller.LoginController">
   <center>
      <AnchorPane prefHeight="300.0" prefWidth="450.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="login" layoutX="76.0" layoutY="120.0" prefHeight="30.0" prefWidth="200.0" promptText="Username" />
            <PasswordField fx:id="password" layoutX="76.0" layoutY="162.0" prefHeight="30.0" prefWidth="200.0" promptText="Password" />
            <Button fx:id="btnlogin" layoutX="126.0" layoutY="208.0" mnemonicParsing="false" onAction="#loginAction" prefHeight="30.0" prefWidth="100.0" text="Login" />
            <Label fx:id="message" alignment="CENTER" contentDisplay="TOP" layoutX="76.0" layoutY="61.0" prefHeight="17.0" prefWidth="200.0" text=" " textFill="#ff4040" textOverrun="CLIP">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
