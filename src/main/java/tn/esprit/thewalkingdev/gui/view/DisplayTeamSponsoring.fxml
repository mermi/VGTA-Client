<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40"
	fx:controller="tn.esprit.thewalkingdev.gui.controller.DisplayTeamSponsoringController">
	<children>
		<AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="630.0"
			prefWidth="738.0">
			<children>
				<TableView fx:id="table" layoutX="69.0" layoutY="203.0"
					prefHeight="276.0" prefWidth="573.0">
					<columns>
						<TableColumn fx:id="lastNameCol" prefWidth="146.0"
							text="Last Name" />
						<TableColumn fx:id="firstNameCol" prefWidth="138.0"
							text="First Name" />
						<TableColumn fx:id="roleCol" prefWidth="133.0" text="Role" />
						<TableColumn fx:id="emailCol" prefWidth="155.0" text="Email" />
					</columns>
				</TableView>
				<Label layoutX="149.0" layoutY="155.0" text="Max number of member">
					<font>
						<Font size="16.0" />
					</font>
				</Label>
				<Label layoutX="220.0" layoutY="37.0" prefHeight="50.0"
					prefWidth="314.0" text=" TEAM SPONSORING" textAlignment="CENTER">
					<font>
						<Font size="30.0" />
					</font>
				</Label>
				<Button fx:id="addMemberBtnId" layoutX="197.0" layoutY="512.0"
					mnemonicParsing="false" onAction="#AddMemberBtnAction" prefHeight="38.0"
					prefWidth="98.0" text="Add member" />
				<Button fx:id="updateNumberId" layoutX="435.0" layoutY="155.0"
					mnemonicParsing="false" onAction="#updateNumberBtnAction"
					prefHeight="20.0" prefWidth="108.0" text="update number" />
				<Button fx:id="deleteMemberBtnId" layoutX="397.0" layoutY="512.0"
					mnemonicParsing="false" onAction="#DeleteMemberBtnAction"
					prefHeight="38.0" prefWidth="108.0" text="Delete member" />
				<TextField fx:id="numberMax" layoutX="345.0" layoutY="155.0"
					prefHeight="25.0" prefWidth="63.0" />
			</children>
		</AnchorPane>
	</children>
</AnchorPane>
